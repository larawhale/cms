{
    "name": "larawhale/cms",
    "description": "A Laravel cms package",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Thomas van der Veen",
            "email": "t.v.d.veen@outlook.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "laravel/framework": "^8.0",
        "laravelcollective/html": "^6.0",
        "laravel/ui": "^3.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.0",
        "orchestra/testbench": "^6.0",
        "orchestra/testbench-browser-kit": "^6.0",
        "orchestra/testbench-dusk": "^6.0",
        "owowagency/laravel-test-response": "^0.0.5",
        "phpunit/phpunit": "^9.0",
        "spatie/phpunit-snapshot-assertions": "^4.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "files": [
            "src/app/Library/helpers.php"
        ],
        "psr-4": {
            "LaraWhale\\Cms\\": "src/app/",
            "LaraWhale\\Cms\\Database\\Factories\\": "src/database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaraWhale\\Cms\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaraWhale\\Cms\\Providers\\CmsServiceProvider"
            ]
        }
    }
}
